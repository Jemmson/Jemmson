{
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test:unit:all": "jest tests/JS/",
    "test:unit:one": "jest",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:unit:debug:one": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:unit:all:watch": "jest --watch tests/JS/",
    "test:unit:one:watch": "jest --watch",
    "test": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/JS/setup.js tests/JS/**/*.spec.js",
    "test_file": "mocha-webpack  --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/JS/setup.js",
    "test_file_glob": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/JS/setup.js",
    "test_file_watch": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --watch --require tests/JS/setup.js",
    "test:integration": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js && ",
    "testst": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js — timeout 10000 --require tests/JS/setup.js",
    "jswatch": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --watch --require tests/JS/setup.js tests/JS/**/*.spec.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@mdi/font": "^4.6.95",
    "@popperjs/core": "^2.2.1",
    "bootstrap": "^4.4.1",
    "dinero.js": "^1.6.0",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "laravel-echo": "^1.6.1",
    "moment": "^2.24.0",
    "popper.js": "^1.16.1",
    "pusher-js": "^5.0.3",
    "stripe": "^8.24.0",
    "sweetalert2": "^9.10.9",
    "underscore": "^1.9.1",
    "urijs": "^1.19.2",
    "v-mask": "^2.0.2",
    "vue": "^2.6.10",
    "vue-element-loading": "^1.1.5",
    "vue-paginate": "^3.6.0",
    "vue-router": "^3.1.3",
    "vue-the-mask": "^0.11.1",
    "vue-toasted": "^1.1.27",
    "vuetify": "^2.1.13",
    "vuetify-loader": "^1.4.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios": "^0.19",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^5.1",
    "cypress": "^4.0.2",
    "jest": "^24.9.0",
    "laravel-mix": "^4.0.7",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.13",
    "lodash.clonedeep": "^4.5.0",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.22.5",
    "sass-loader": "7.*",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  }
}
